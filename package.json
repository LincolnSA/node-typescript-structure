{
  "name": "node-typescript-structure",
  "version": "1.0.0",
  "main": "index.js",
  "author": "lincoln silva <jllincolnsilva@gmail.com>",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpileOnly --ignore-watch node_modules --no-notify src/server.ts",
